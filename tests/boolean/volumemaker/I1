# test script on make volume operation
# cylinder plane

# planar face 
plane pln_f1 635 431.80000000000001 -139.69999999999999 0 1 0
erase pln_f1
mkface f1 pln_f1 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f2 1016 127 -139.69999999999996 1 0 1.1102230246251565e-016
erase pln_f2
mkface f2 pln_f2 -1000000 1000000 -1000000 1000000

# cylindrical face 
cylinder cyl_f3 635 126.99999999999999 -1.4210854715202004e-014 0 -1.2246467991473532e-016 -1 419.10000000000002
erase cyl_f3
mkface f3 cyl_f3 0 6.2831853071795862 -1000000 1000000

# planar face 
plane pln_f4 635 127 177.80000000000001 1.1102230246251565e-016 0 -1
erase pln_f4
mkface f4 pln_f4 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f5 635 -177.80000000000001 -139.69999999999999 0 1 0
erase pln_f5
mkface f5 pln_f5 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f6 254 127 -139.70000000000002 1 0 1.1102230246251565e-016
erase pln_f6
mkface f6 pln_f6 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f7 635 126.99999999999996 -254 0 1.2246467991473532e-016 1
erase pln_f7
mkface f7 pln_f7 -1000000 1000000 -1000000 1000000

# make volume operation 
mkvolume result f1 f2 f3 f4 f5 f6 f7

set square 4.544e+006

