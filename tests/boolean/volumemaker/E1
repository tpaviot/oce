# test script on make volume operation
# cylinder plane

# planar face 
plane pln_f1 -306.53078964627537 -1038.2137499999999 -251.37646071372467 -0.70710678118654746 4.4408920985006262e-016 0.70710678118654768
erase pln_f1
mkface f1 pln_f1 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f2 -27.577164468182303 -1038.2137499999999 -27.577164468182389 0.70710678118654768 2.2204460492503131e-016 0.70710678118654746
erase pln_f2
