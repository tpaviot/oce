# cylindrical face 
cylinder cyl_f3 0 0 7.5439654523279387e-014 0 1 1.1102230246251565e-016 260
erase cyl_f3
mkface f3 cyl_f3 0 6.2831853071795862 -1000000 1000000

# planar face 
plane pln_f4 -251.37646071372464 -1038.2137499999999 -306.53078964627542 -0.70710678118654746 4.4408920985006262e-016 0.70710678118654768
erase pln_f4
