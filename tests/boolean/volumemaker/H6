# cylindrical face 
cylinder cyl_f7 0 0 -2.0206059048177849e-013 0 -1 1.1102230246251565e-016 1650
erase cyl_f7
mkface f7 cyl_f7 0 6.2831853071795862 -1000000 1000000

# make volume operation 
mkvolume result f1 f2 f3 f4 f5 f6 f7

set square 3.20115e+007

