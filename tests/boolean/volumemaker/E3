# test script on make volume operation
# cylinder plane

# planar face 
plane pln_f1 0 -1398 -8.3266726846886741e-016 0 1 1.1102230246251565e-016
erase pln_f1
mkface f1 pln_f1 -1000000 1000000 -1000000 1000000

# cylindrical face 
cylinder cyl_f2 0 0 1.5654144647214707e-013 0 1 1.1102230246251565e-016 1750
erase cyl_f2
mkface f2 cyl_f2 0 6.2831853071795862 -1000000 1000000

# planar face 
plane pln_f3 -837.85723364880107 -1532.5 -1479.1857140209454 -0.89100652418836812 -2.2204460492503136e-016 0.45399049973954653
erase pln_f3
mkface f3 pln_f3 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f4 -771.03174431000002 -1532.5 -1434.59193091 -2.2204460492503131e-016 1.1102230246251565e-016 -1
erase pln_f4
mkface f4 pln_f4 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f5 0 -1450 -4.4408920985006262e-015 0 1 1.1102230246251565e-016
erase pln_f5
mkface f5 pln_f5 -1000000 1000000 -1000000 1000000

# planar face 
plane pln_f6 -793.75341161103245 -1532.5000000000002 -1557.8287844128681 0.45399049973954719 1.1102230246251564e-016 0.89100652418836768
erase pln_f6
mkface f6 pln_f6 -1000000 1000000 -1000000 1000000

# cylindrical face 
cylinder cyl_f7 0 0 -1.8139656443594276e-013 0 -1 1.1102230246251565e-016 1720
erase cyl_f7
mkface f7 cyl_f7 0 6.2831853071795862 -1000000 1000000

# make volume operation 
mkvolume result f1 f2 f3 f4 f5 f6 f7

set square 2.15489e+007

