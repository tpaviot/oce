set square 278545

set nbshapes_expected "
Number of shapes in shape
 VERTEX    : 146
 EDGE      : 226
 WIRE      : 111
 FACE      : 97
 SHELL     : 7
 SOLID     : 7
 COMPSOLID : 0
 COMPOUND  : 1
 SHAPE     : 595
"
checknbshapes result -ref ${nbshapes_expected} -t -m "Boolean operations on shapes with fillets"

set 3dviewer 1
