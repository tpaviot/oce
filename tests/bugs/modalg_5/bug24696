puts "========="
puts "OCC24696"
puts "========="
puts ""
###########################################################
# Lower performance of the new Edge/Edge intersection algorithm
###########################################################

pload QAcommands

dchrono h reset
dchrono h start

restore [locate_data_file bug24696_cx_e1200_nurbs.brep] cx

bclearobjects
bcleartools

set edges [explode cx e]
set nbe [llength $edges]
for {set i 1} {$i <= $nbe} {incr i} {baddobjects cx_$i}
bfillds
bbuild result

dchrono h stop
set q [dchrono h show]

regexp {CPU user time: ([-0-9.+eE]+) seconds} $q full z
puts "$z"

if { [regexp {Debug mode} [dversion]] } {
  if { [regexp {Windows} [dversion]] } {
    set max_time 100.0
  } else {
    set max_time 200.0
  }
} else {
  if { [regexp {Windows} [dversion]] } {
    set max_time 25.0
  } else {
    set max_time 40.0
  }
}

if { [regexp {Mac OS X} [dversion]] } {
  set max_time 100.0
}

if { $z > ${max_time} } {                                         
    puts "Elapsed time is more than ${max_time} seconds - Faulty"
} else {
    puts "Elapsed time is less than ${max_time} seconds - OK"
}

set 2dviewer 1
