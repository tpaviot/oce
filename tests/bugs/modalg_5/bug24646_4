bfillds
bbuild result

set square 3.1449e+06

checknbshapes result -vertex 22 -edge 38 -wire 20 -face 18 -shell 3 -solid 3 -compsolid 0 -compound 1 -shape 105 -t -m "Result done by Boolean Operation algorithm"

set 2dviewer 1
