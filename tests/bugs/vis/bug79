puts "========"
puts "OCC79"
puts "========"

vinit
restore [locate_data_file OCC162.brep] s
vdisplay s

verase s
vdisplay s

set listmem {}
set i_max 10
for {set i 1} {${i} <= ${i_max}} {incr i} {
    verase s
    vdisplay s

    lappend listmem [meminfo h]
    checktrend $listmem 0 1 "Memory leak detected"
}
