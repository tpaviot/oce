puts "========"
puts "OCC25611"
puts "========"
puts ""
############################################################################
puts "Displaying \'zbufftrihedron\' interferes with \'vgraduatedtrihedron\'"
############################################################################

set anImage $imagedir/${casename}.png

vinit View1
psphere s 10
vdisplay s
vaxo
vfit
vsetdispmode s 1
vgraduatedtrihedron -on
vzbufftrihedron

vdump $anImage
vclear
vclose View1 

