puts "================"
puts "BUC60632"
puts "================"

vinit
BUC60632 0 100
vfit
vmoveto 126 143
puts "Highlighting by 'Move To' command in wireframe mode"

set x_coord 129
set y_coord 143

checkcolor $x_coord $y_coord 0 1 1

if { $stat != 1 } {
   puts "Error : Highlighting by Move To command in wireframe mode is WRONG."
}
set only_screen 1


